//用户信息的全选与取消
function userCheckAll(){
	$("#checkall").click(
		function(){
			if(this.checked){
				$("input[name='items']").each(function(){this.checked=true;});
			}else{
				$("input[name='items']").each(function(){this.checked=false;});
			}
		}
	);
}


function initUcuser(){
	loadUcuser(true);
}

function loadUcuser(isFilter_uid_plat){
	if(ucuser){
		$(ucuser).each(function(){
			if(!(isFilter_uid_plat && (this.value == "_uid" || this.value == "_plat"))){
				var html = '<label class="checkbox-inline" title="'+this.valueName+'('+this.value+')">' +
						'<input type="checkbox" valuename="' + this.valueName + '" value="' + this.value + '" name="items">' + this.show+"("+this.value+")" + 
					'</label>';
				$("#ucuser").append(html);	
			}
		});
	};
	userCheckAll();
	$('[data-toggle="tooltip"]').tooltip();
}
